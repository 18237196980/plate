
















































/* 分页样式 */
.page-group {
	background: #fff;
	margin-top: 20rpx;
	margin-bottom: 20rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: start;
	-webkit-justify-content: flex-start;
	        justify-content: flex-start;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.page-group view {
	height: 108rpx;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	font-size: 29rpx;
	color: #333;
	text-align: center;
	line-height: 108rpx;
}
.page-group .disabled {
	color: #ccc;
}

